<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kunize.uswtimetable">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".TimeTableSelPref"
        android:allowBackup="true"
        android:icon="@mipmap/ic_main"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_main_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Uswtimetable">
        <activity android:name=".StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".TimeTableListActivity" />
        <activity android:name=".ClassInfoActivity"
            android:windowSoftInputMode="adjustPan"/>
        <activity android:name=".AddClassActivity"
            android:windowSoftInputMode="adjustPan"/>
        <activity android:name=".CreateTimeTableActivity"
            android:windowSoftInputMode="adjustPan"/>
        <activity android:name=".MainActivity">

        </activity>
    </application>

</manifest>